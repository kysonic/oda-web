{
  "presets": ["next/babel"],
  "plugins": [
    [
      "module-resolver",
      {
        "root": ["./"],
        "alias": {
          "@components": "./src/components",
          "@config": "./src/config",
          "@pages": "./src/pages",
          "@services": "./src/services",
          "@stores": "./src/stores",
          "@themes": "./src/themes",
          "@utils": "./src/utils"
        },
        "extensions": [".js", ".jsx", ".tsx"]
      }
    ]
  ]
}
