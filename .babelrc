{
  "presets": ["next/babel"],
  "plugins": [
    [
      "module-resolver",
      {
        "root": ["./"],
        "alias": {
          "@components": "./src/components",
          "@layouts": "./src/components/layouts",
          "@config": "./src/config",
          "@pages": "./src/pages",
          "@services": "./src/services",
          "@styles": "./src/styles",
          "@utils": "./src/utils",
          "@types": "./src/types",
          "@i18n": "./src/i18n",
          "@graphql": "./src/graphql",
          "@hooks": "./src/hooks",
          "@svg": "./public/svg"
        },
        "extensions": [".js", ".jsx", ".tsx", ".svg"]
      }
    ],
    ["@babel/plugin-proposal-decorators", {
      "legacy": true
    }]
  ]
}
